{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/system-prod-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "EmailSettings": {
    "Server": "smtp.gmail.com",
    "Port": 587,
    "SenderName": "Stanza Team",
    "SenderEmail": "REPLACE_ME_SENDER_EMAIL",
    "Username": "REPLACE_ME_USERNAME",
    "Password": "REPLACE_ME_PASSWORD",
    "BaseUrl": "https://stanza-events.com"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=REPLACE_ME_SERVER;Database=EventPlanningDb;User Id=REPLACE_ME_USER;Password=REPLACE_ME_PASSWORD;TrustServerCertificate=True;"
  }
}
