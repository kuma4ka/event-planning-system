{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/system-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "EmailSettings": {
    "Server": "smtp.gmail.com",
    "Port": 587,
    "SenderName": "Stanza Team",
    "SenderEmail": "",
    "Username": "",
    "Password": "",
    "BaseUrl": "https://localhost:7073"
  },
  "ConnectionStrings": {},
  "CountrySettings": {
    "FilePath": "countries.json"
  }
}